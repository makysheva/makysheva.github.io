extends ../../base
include ../../blocks/headers-footers/menu/menu

block title
    | Headers & Footers

block main
    .logo-wrapper
        include ../../blocks/logo/logo-grey

block headers-section
    .container
        .header__wrapper
            .header__item--register
                .logo-wrapper
                    include ../../blocks/logo/logo-main
                .header__menu--register
                    .menu
                        ul.menu__list
                            -
                                let menuList = [
                                    {title: "О нас", link: "#", showFooter: true,},
                                    {title: "Услуги", link: "#",showFooter: true,
                                        subnav: [{title: "item 1",link: "#",}, {title: "item 2",link: "#",}]},
                                    {title: "Вакансии", link: "#", showFooter: true,},
                                    {title: "Новости", link: "#", showFooter: true,},
                                    {title: "Соглашения", link: "#", showFooter: true,
                                        subnav: [{title: "item 1", link: "#",},{title: "item 2",link: "#",}]},
                                ]

                        each element in menuList
                            li.menu__item
                                +elementMenu(element)



